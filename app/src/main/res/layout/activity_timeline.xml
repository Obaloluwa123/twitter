<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LogoutButton"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cardview_dark_background"
    tools:context=".TimelineActivity">

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/swipeContainer"
        android:layout_width="match_parent"
        android:layout_height="667dp">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvTweets"
            android:layout_width="397dp"
            android:layout_height="660dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="9dp"
            android:layout_marginTop="2dp" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_vector_messages" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_vector_messages" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_vector_messages_stroke" />
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>


    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="680dp"
        android:layout_marginEnd="16dp"
        android:onClick="onLogoutButton"
        android:text="Logout"
        tools:ignore="OnClick" />


    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="58dp"
        android:layout_height="57dp"
        android:layout_below="@+id/swipeContainer"
        android:layout_alignParentStart="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="11dp"
        app:srcCompat="@drawable/ic_vector_home" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="59dp"
        android:layout_height="53dp"
        android:layout_below="@+id/swipeContainer"
        android:layout_marginStart="22dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/imageButton3"
        app:srcCompat="@drawable/ic_vector_search" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="58dp"
        android:layout_height="56dp"
        android:layout_below="@+id/swipeContainer"
        android:layout_marginStart="36dp"
        android:layout_marginTop="6dp"
        android:layout_toEndOf="@+id/imageButton5"
        app:srcCompat="@drawable/ic_vector_notifications" />
</RelativeLayout>